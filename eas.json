{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "store",
      "channel": "preview",
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_ENV": "preview"
      }
    },
    "preview-apk": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_ENV": "preview"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "preview": {
      "ios": {
        "ascAppId": "6758100638"
      }
    },
    "production": {
      "ios": {
        "ascAppId": "6758100638"
      },
      "android": {
        "releaseStatus": "draft"
      }
    }
  }
}
